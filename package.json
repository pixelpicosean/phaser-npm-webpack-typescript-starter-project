{
  "name": "phaser-starter",
  "productName": "my-awesome-game",
  "version": "1.0.0",
  "main": "electron-main.js",
  "scripts": {
    "webpack:dev": "webpack --config webpack.dev.config.js --progress --colors",
    "webpack:dist": "webpack --config webpack.dist.config.js --progress --colors",
    "build:dev": "echo-cli 'Building DEV ...' && npm run webpack:dev && echo-cli 'Build Complete ✔'",
    "build:dist": "echo-cli 'Building DIST ...' && npm run webpack:dist && echo-cli 'Build Complete ✔'",
    "server:dev": "webpack-dev-server --config webpack.dev.config.js --open",
    "server:dist": "webpack-dev-server --config webpack.dist.config.js --open",
    "assets:dev": "node ./scripts/generate_assets_data.js --dev",
    "assets": "node ./scripts/generate_assets_data.js",
    "electron:dev": "npm run build:dev && electron .",
    "electron:dist": "npm run build:dist && electron .",
    "electron:pack:dev": "npm run build:dev && node ./scripts/package_electron_app.js",
    "electron:pack:dist": "npm run build:dist && node ./scripts/package_electron_app.js",
    "check_deps_versions": "npm-check --skip-unused -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixelpicosean/phaser-starter.git"
  },
  "author": "Sean Bohan <https://github.com/pixelpicosean>",
  "devDependencies": {
    "@types/node": "^7.0.18",
    "clean-webpack-plugin": "^0.1.16",
    "commander": "^2.9.0",
    "copy-webpack-plugin": "^4.0.1",
    "echo-cli": "^1.0.8",
    "electron": "^1.6.7",
    "electron-packager": "^8.7.0",
    "expose-loader": "^0.7.3",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "npm-check": "^5.4.4",
    "phaser-ce": "^2.7.9",
    "pngjs-image": "^0.11.7",
    "shelljs": "^0.7.7",
    "ts-loader": "^2.0.3",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.2",
    "webfontloader": "^1.6.27",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-shell-plugin": "^0.5.0",
    "xml2js": "^0.4.17"
  }
}
